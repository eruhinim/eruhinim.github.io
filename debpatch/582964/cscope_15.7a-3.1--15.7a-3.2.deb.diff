diff -Nru cscope-15.7a/debian/cscope-el.emacsen-install cscope-15.7a/debian/cscope-el.emacsen-install
--- cscope-15.7a/debian/cscope-el.emacsen-install	1970-01-01 02:00:00.000000000 +0200
+++ cscope-15.7a/debian/cscope-el.emacsen-install	2010-11-13 16:08:32.000000000 +0200
@@ -0,0 +1,28 @@
+#!/bin/sh
+#
+# emacsen install script for the Debian GNU/Linux cscope package
+# based on the emacsen install script of the cmake package
+
+FLAVOUR=$1
+PACKAGE=cscope
+
+ELDIR=/usr/share/emacs/site-lisp/
+ELCDIR=/usr/share/$FLAVOUR/site-lisp/$PACKAGE
+ELFILE="xcscope.el"
+FLAGS="-batch -no-site-file -l path.el -f batch-byte-compile"
+
+if [ $FLAVOUR != emacs ]; then
+	echo "install/$PACKAGE: Byte-compiling for $FLAVOUR"
+
+	install -m 755 -d $ELCDIR
+	cd $ELDIR
+	install -m 644 $ELFILE $ELCDIR
+
+	cd $ELCDIR
+	cat << EOF > path.el
+(setq load-path (cons "." load-path))
+(setq  byte-compile-warnings nil)
+EOF
+	$FLAVOUR $FLAGS $ELFILE
+	rm -f $ELFILE path.el
+fi
